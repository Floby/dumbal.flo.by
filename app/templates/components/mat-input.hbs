{{input
  type=type
  id=(concat elementId '-input')
  name=(concat elementId '-input')
  min=min
  max=max
  autocomplete=autocomplete
  placeholder=placeholder
  maxlength=maxlength
  focus-out=onBlur
  value=value}}
{{#if label}}
  <label 
    for="{{concat elementId '-input'}}"
    class="{{if hasValue "not-empty"}}"
  >
    {{label}}
  </label>
{{/if}}
<br>
